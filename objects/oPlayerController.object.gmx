    var m_pad = m_size * 2 / 5;
    var m_outline = 2;
    draw_set_alpha(m_a)
    draw_roundrect_color(m_x,m_y,m_x + m_w,m_y + m_size * 2 + m_pad * 3,m_bc,m_bc,0)
