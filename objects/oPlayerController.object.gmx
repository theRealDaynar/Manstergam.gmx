    //draw monster sprite background
    for(var yy = 0; yy &lt; 2; yy++)
        for(var xx = 0; xx &lt; 4; xx++)
        {
            if mouse_x &gt; m_x + (xx + 1) * m_pad + xx * m_size and mouse_y &gt; m_y + (yy + 1) * m_pad + yy * m_size and mouse_x &lt; m_x + (xx + 1) * m_pad + (xx + 1) * m_size and mouse_y &lt; m_y + (yy + 1) * m_pad + (yy + 1) * m_size
