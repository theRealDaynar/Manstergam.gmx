            else
                inner_color = m_c
            if xx &lt; 2
                draw_roundrect_colour(m_x + (xx + 1) * m_pad + xx * m_size - m_outline * 2,m_y + (yy + 1) * m_pad + yy * m_size - m_outline * 2,m_x + (xx + 1) * m_pad + (xx + 1) * m_size + m_outline * 2,m_y + (yy + 1) * m_pad + (1 + yy) * m_size + m_outline * 2,m_hc,m_hc,0)
