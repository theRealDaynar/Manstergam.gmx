            for(var xx = 0; xx &lt; 2; xx ++)
            {
                with(oBattleController.team_1[xx,yy])
                {
                    if other.monsters[xx,yy] != noone
                    {
                        team_num ++
                        monster_init(other.monsters[xx,yy])
